{
  "name": "Auto Pickup",
  "manifest_version": 2,
  "version": "1.0",
  "permissions": [
    "https://*.twitter.com/"
  ],
  "browser_action": {
    "default_icon": "128.png",
    "default_title": "Auto Pickup"
  },
  "content_scripts": [
    {
      "matches": ["https://*.twitter.com/*"],
      "css": ["style.css"],
      "js": ["index.js"]
    }
  ],
  "icons": {
    "128": "128.png"
  }
}